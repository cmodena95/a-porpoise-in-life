<%= simple_form_for(@porpoise) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :species %>
  <%= f.text_field :species %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.file_field :photo %>
  <%= f.submit %>
<% end %>